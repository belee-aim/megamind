TENANT_ID="aimlink"

# LLM Provider Configuration
# PROVIDER options: GEMINI, DEEPSEEK, CLAUDE
PROVIDER="GEMINI"
API_KEY="your_api_key_here"

# Model examples by provider:
# GEMINI: "gemini-2.5-flash", "gemini-1.5-pro"
# DEEPSEEK: "deepseek-chat"
# CLAUDE: "claude-sonnet-4-5-20250929", "claude-haiku-4-5-20251001", "claude-opus-4-1-20250805"
MODEL="gemini-2.5-flash"

# Embedding model (GEMINI recommended, CLAUDE not supported)
EMBEDDING_MODEL="models/embedding-001"

# Legacy Gemini configuration (for backward compatibility)
GOOGLE_API_KEY="your_google_api_key_here"

# Frappe/ERPNext Configuration
FRAPPE_URL="your_frappe_url_here"
FRAPPE_API_KEY="your_frappe_api_key_here"
FRAPPE_API_SECRET="your_frappe_api_secret_here"
SUPABASE_URL="your_supabase_url_here"
SUPABASE_KEY="your_supabase_key_here"
SUPABASE_CONNECTION_STRING="your_supabase_connection_string_here"
LANGSMITH_API_KEY="your_langsmith_api_key_here"
LANGSMITH_PROJECT=pr-cooked-evaluator-52
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LLAMA_CLOUD_API_KEY="your_llama_cloud_api_key_here"
FRAPPE_MCP_SERVER_PATH="/home/skele/code/frappe_mcp_server/build/index.js"
MINION_API_URL="http://localhost:8000"
TITAN_API_URL="http://localhost:8001"
FRAPPE_AUTH_MODE=jwt

# Sentry Configuration (optional - for error tracking and monitoring)
SENTRY_DSN=
ENVIRONMENT="development"  # Options: development, staging, production
SENTRY_TRACES_SAMPLE_RATE=1.0  # Performance monitoring: 1.0=100% (dev), 0.1=10% (prod) to reduce overhead

# Firebase Realtime Database Configuration
FIREBASE_DATABASE_URL="https://your-project-default-rtdb.firebaseio.com"
# To generate FIREBASE_CREDENTIALS_BASE64:
# Linux/Mac: base64 -w 0 firebase-service-account.json
# Or: cat firebase-service-account.json | base64 -w 0
FIREBASE_CREDENTIALS_BASE64="base64_encoded_service_account_json_here"